{
  "name": "trejjam/sentry",
  "description": "Sentry integration into Nette framework",
  "keywords": [
    "nette",
    "contributte",
    "sentry",
    "php"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/trejjam/sentry",
  "authors": [
    {
      "name": "Jan Trejbal",
      "homepage": "https://github.com/trejjam"
    }
  ],
  "require": {
    "php": "^8.2 || ^8.3",
    "nette/di": "^3.2.3",
    "nette/schema": "^1.3.2",
    "sentry/sdk": "^4.0.0"
  },
  "require-dev": {
    "contributte/logging": "^0.6.3",
    "http-interop/http-factory-guzzle": "^1.2.0",
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3",
    "nette/tester": "^2.5.1",
    "phpstan/extension-installer": "^1.4.1",
    "phpstan/phpstan": "^2.0.4",
    "phpstan/phpstan-deprecation-rules": "^2.0.1",
    "phpstan/phpstan-nette": "^2.0.1",
    "phpstan/phpstan-strict-rules": "^2.0.1"
  },
  "suggest": {
    "http-interop/http-factory-guzzle": "Recomended Guzzle HTTP Factory",
    "contributte/logging": "To use SentryLogger"
  },
  "autoload": {
    "psr-4": {
      "Trejjam\\Sentry\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  }
}
