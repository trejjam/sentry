{
  "name": "trejjam/sentry",
  "description": "Sentry integration into Nette framework",
  "keywords": [
    "nette",
    "contributte",
    "sentry",
    "php"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/trejjam/sentry",
  "authors": [
    {
      "name": "Jan Trejbal",
      "homepage": "https://github.com/trejjam"
    }
  ],
  "require": {
    "php": "^7.4 | ^8.0",
    "nette/di": "^2.4 | ^3.1",
    "sentry/sdk": "^3.1"
  },
  "require-dev": {
    "http-interop/http-factory-guzzle": "^1.0",

    "ninjify/nunjuck": "^0.4",
    "ninjify/qa": "^0.12",

    "phpstan/phpstan": "^0.12",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-nette": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12"
  },
  "suggest": {
    "http-interop/http-factory-guzzle": "Recomended Guzzle HTTP Factory",
    "contributte/logging": "To use SentryLogger"
  },
  "autoload": {
    "psr-4": {
      "Trejjam\\Sentry\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
